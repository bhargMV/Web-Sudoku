<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    
    <servlet>
        <servlet-name>SudokuCreate</servlet-name>
        <servlet-class>SudokuCreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginVerification</servlet-name>
        <servlet-class>LoginVerification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SudokuSolve</servlet-name>
        <servlet-class>SudokuSolve</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SudokuCreate</servlet-name>
        <url-pattern>/Sudoku_Create</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>LoginVerification</servlet-name>
        <url-pattern>/LoginVerification</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>SudokuPlay</servlet-name>
        <jsp-file>/WEB-INF/SudokuPlay.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>SudokuPlay</servlet-name>
        <url-pattern>/Sudoku_Play</url-pattern>
    </servlet-mapping>
    
    
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
   
    <servlet-mapping>
        <servlet-name>SudokuSolve</servlet-name>
        <url-pattern>/Sudoku_Solve</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
        
    </web-app>
